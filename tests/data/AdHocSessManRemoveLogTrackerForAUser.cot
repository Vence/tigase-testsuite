send: {
<iq id="remove-user-tracker"
    from="$(from-jid)"
    to="$(to-jid)"
    type="set">
    <command xmlns="http://jabber.org/protocol/commands"
             node="http://jabber.org/protocol/admin#remove-user-tracker" />
</iq>
}


expect: {
<iq id="remove-user-tracker"
    from="$(to-jid)"
    to="$(from-jid)"
    type="result">
    <command
        xmlns="http://jabber.org/protocol/commands"
        node="http://jabber.org/protocol/admin#remove-user-tracker"
        status="executing">
		<x xmlns="jabber:x:data" type="form">
			<title>Removing a User Log Tracker</title>
			<field type="hidden" var="FORM_TYPE">
				<value>http://jabber.org/protocol/admin</value>
			</field>
			<field type="jid-single" label="The Jabber ID for the tracker to be removed" var="accountjid">
				<value/>
			</field>
		</x>
		<actions execute="complete">
			<complete/>
		</actions>
    </command>
</iq>
}

send: {
<iq id="remove-user-tracker"
    from="$(from-jid)"
    to="$(to-jid)"
    type="set">
    <command
        xmlns="http://jabber.org/protocol/commands"
        node="http://jabber.org/protocol/admin#remove-user-tracker">
		<x xmlns="jabber:x:data" type="submit">
			<field type="hidden" var="FORM_TYPE">
				<value>http://jabber.org/protocol/admin</value>
			</field>
			<field type="jid-single" var="accountjid">
				<value>$(cdata)</value>
			</field>
		</x>
    </command>
</iq>
}

expect: {
<iq id="remove-user-tracker"
    from="$(to-jid)"
    to="$(from-jid)"
    type="result">
    <command xmlns="http://jabber.org/protocol/commands"
        status="completed"
        node="http://jabber.org/protocol/admin#remove-user-tracker">
		<x xmlns="jabber:x:data" type="result">
			<field type="fixed" var="Note">
				<value></value>
			</field>
		</x>
    </command>
</iq>
}