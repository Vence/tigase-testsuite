send: {
<iq type="set" to="vhost-man@$(hostname)" id="aacba">
	<command xmlns="http://jabber.org/protocol/commands" node="VHOSTS_REMOVE">
		<x xmlns="jabber:x:data" type="submit">
			<field type="text-single" var="VHost"><value>$(cdata)</value></field>
		</x>
	</command>
</iq>
}
expect: {
<iq from="vhost-man@$(hostname)" type="result" to="$(from-jid)" id="aacba">
	<command xmlns="http://jabber.org/protocol/commands" status="completed" node="VHOSTS_REMOVE">
		<x xmlns="jabber:x:data" type="result">
			<field type="fixed" var="Note">
			</field>
		</x>
	</command>
</iq>
}
